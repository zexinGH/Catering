/*Tab样式*/
#menutab1 ul{
	margin:0px;
	padding-top: 0px;
	padding-right: 0px;
	padding-bottom: 0px;
	padding-left: 0px;
	}
#menutab1 li{ 
	display:inline;
	}
#menutab1 a{ 
	float:left; 
	background:url(../images/core/tableft11.gif) left top no-repeat; 
	padding-left:6px;
	}
#menutab1 a:hover{
	float:left;
	padding-left:6px;
	background-image: url(../images/core/tableft11.gif);
	background-repeat: no-repeat;
	background-position: left -29px;
	text-decoration:none;
	}
#menutab1 span{	
	text-align: center;
	display:block;
	background:url(../images/core/tabright11.gif) right top no-repeat;
	margin-right: 2px;
	padding-top: 4px;
	padding-bottom: 4px;
	padding-left: 6px;
	padding-right: 10px;
	}
#menutab1 a:hover span{
	display:block;
	margin-right: 2px;
	padding-top: 4px;
	padding-bottom: 4px;
	padding-left: 6px;
	background-image: url(../images/core/tabright11.gif);
	background-repeat: no-repeat;
	background-position: right -29px;
	text-decoration:none;
	color:#000;
	padding-right: 10px;
	}
.menubg #menutab1 {
	margin-left: 10px;
	}
.menubg {
	background-image: url(../images/core/labbg.gif);
	background-repeat: repeat-x;
	height: 29px;
	line-height:29px;
	}

