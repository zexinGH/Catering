/*--CSS 重置 开始--*/
body, div, dl, dt, dd, ul, ol, li,h1, h2, h3, h4, h5, h6, pre, code,form, fieldset, legend, input, button,textarea, p, blockquote, th, td {margin: 0;padding: 0;font-size:12px; }
fieldset, img {border: 0;}
:focus {outline: 0;}
address, caption, cite, code, dfn,
em, strong, th, var, optgroup {font-style: normal;font-weight: normal;}
h1, h2, h3, h4, h5, h6 {font-weight: normal;}
abbr, acronym { border: 0;font-variant: normal;}
input, button, textarea,
select, optgroup, option {font-family: inherit; font-style: inherit;font-weight: inherit;}
input, button, textarea, select {*font-size:12px;}
body {line-height: 1.5;}
ol, ul {list-style: none;}
ul li{ margin:0; padding:0;}
table {border-collapse: collapse;border-spacing: 0; margin:0 auto;}
caption, th {text-align: left;}
:link, :visited , ins {text-decoration: none;}
blockquote, q {quotes: none;}
blockquote:before, blockquote:after,
q:before, q:after {content: '';content: none;}
/*--CSS 重置 结束--*/


/*--通用Class 开始--*/
	/*--通用定位--*/
	.floatLeft{ float:left;}/*向左浮动*/
	.floatRight{ float:right;}/*向右浮动*/
	.clear{clear:both;height:1;background-color:transparent;overflow:hidden;line-height:1px;}/*两侧清除*/
	.hideFocus{outline:none;}/*hide-focus:expression(this.hideFocus=true);去除a标签虚线框*/ 
	.textLeft{ text-align:left;}/*文字向左对齐*/
	.textRight{ text-align:right;}/*文字向右对齐*/
	.textCenter{ text-align:center;}/*文字居中对齐*/
	.indent10{ text-indent:10px;}/*文字缩进10像素*/
	.marginLeft3px{ margin-left:3px !important}
	.verticalTop {vertical-align: text-top;}/*文本中线对齐*/

	/*--字体样式--*/
	.fB{ font-weight:bold;}/*粗体*/
	.fU{ text-decoration:underline;}/*斜体*/
	.f12px{ font-size:12px;}/*12号字体*/
	.f14px{ font-size:14px;}/*14号字体*/
	.f16px{ font-size:16px;}/*16号字体*/
	.f18px{ font-size:18px;}/*18号字体*/
		
	/*--间距样式--*/
	.blank60 {margin-top: 30px;margin-bottom: 30px;}/*60像素行间距*/
	.blank20 {margin-top: 10px;margin-bottom: 10px;height: 20px;}/*20像素行间距*/
	.blank10 {margin-top: 5px;margin-bottom: 5px;}/*10像素行间距*/
	.height40px{ height:40px; width:100%;}
	/*--其它--*/
	.pointer{ cursor:pointer;}/*鼠标手指示状态*/
	.nowrap {white-space:nowrap;}/*强制不折行*/
	em{ margin-left:5px;}
		
	/*--按钮*--/
	html>body .BtnA{ padding:0 10px; outline:none; }/*FF, Opera*/
	.Btn:focus::-moz-focus-inner{border-color:transparent;}/*FF去按钮虚线框*/

	/*--链接样式  命名规则 比如： “.cGray ” c代表颜色 Gray代表颜色名--*/
	A {color:#336699;text-decoration: none;}
	A:visited {color: #336699; text-decoration: none;}
	A:hover {color: #4F6AD7; text-decoration: underline;}
	A:active {color: #ba2636;}

	.cGray {color: #585858;}
	.cGray:visited {color: #585858}
	.cGray A {color: #585858;}
	.cGray A:visited {color: #585858;}
	.cDGray {color: #727171!important;}
	.cDGray:visited {color: #727171;}
	.cDGray A {color: #727171;}
	.cDGray A:visited {color: #727171;}
	.cRed {color: red;}
	.cRed:visited {color: red;}
	.cRed A {color: red;}
	.cRed A:visited {color: red;}
	.cDRed{color:#e60012;}
	.cYellow {color:#FFFF00;}
	.cGreen{ color:#5FA207;}
/*--通用Class 开始--*/
