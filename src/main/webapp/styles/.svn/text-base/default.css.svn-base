/* 全局CSS定义 */
*{ font:"宋体"; font-size:12px; margin:0; padding:0;}
body{
	font-family:"宋体","Arial";
	font-size:12px;
	color:#000;
	margin-top: 0;
	margin-right: auto;
	margin-bottom: 0;
	margin-left: auto;
	}
table{
	font-family:"宋体","Arial";
	font-size:12px;
	color:#000;
	 margin:0 auto;
	}
li{list-style-type:none;}
table{
}
img{border: none;}
textarea{ overflow:auto;}
h2 {
	color: #000;
	margin: 0px;
	padding: 0px 15px;
	font-size: 12px;
	font-weight: bold;
	}

/* 链接颜色 */
a:link {color: #000000; text-decoration:none;}
a:visited {text-decoration:none;}
a:hover {color: #bc2931; text-decoration:underline;}
a:active {color: #bc2931;}
a:active, a:focus { outline:none; }

/* 字体属性 [定义规则，小写f加属性名称] */
.fB {font-weight: bold;}
.fu { text-decoration:underline;}

/* 字体大小*/
.f12px{ font-size:12px;}
.f14px{ font-size:14px;}

/* 文本对齐方式*/
.textRight {text-align: right;!important}
.textLeft {text-align: left;!important}
.textCenter{ text-align:center;!important}
.left2{ margin-left:2px;}

/* 文本颜色*/
.fcred{ color:#FF0000;}
.fcyellow{ color:#FFFF00;}

/* 其他属性 */
.left{ float: left;}
.right{ float: right;}
.clear{ clear: both;}
.indent{ text-indent:20px;}
.indent8{ text-indent:8px;}
.blank4{ height:4px; font-size:1px;  clear:both;overflow:hidden;}
.blank10{ height:10px; font-size:1px;  clear:both;overflow:hidden;}

