/*登录界面样式*/
.loginTb {
	line-height: 22px;
	background-image: url(/images/login/login3_03.gif);
	background-repeat: no-repeat;
	height: 146px;
	width: 339px;
	}
.loginTb a{
	margin-right:10px;
}
.playInput03 {
	background-repeat: no-repeat;
	height: 24px;
	width: 65px;
	border:none;
	background: url(/images/login/login00.gif);
	}
.playInput03:hover{
	background-repeat: no-repeat;
	height: 24px;
	width: 65px;
	border:none;
	background-image: url(/images/login/login_hover.gif);
	}
.playInput04 {
	background-repeat: no-repeat;
	height: 24px;
	width: 65px;
	border:none;
	background: url(/images/login/reset00.gif);
	}
.playInput04:hover {
	background-repeat: no-repeat;
	height: 24px;
	width: 65px;
	border:none;
	background: url(/images/login/reset_hover.gif);
	}
.PicL {
	background-image: url(/images/login/digital.gif);
	background-repeat: no-repeat;
	float: left;
	height: 162px;
	width: 327px;
	position: absolute;
	right: 0px;
	bottom: 40px;
	}
.CC{ 
	text-align:center;
	}

.softphoneArea {
	height: 29px;
	margin-top: 6px;
	margin-left: 260px;
	float: left;
	width: auto;
	margin-right: auto;
	margin-bottom: auto;
	overflow:hidden;
	white-space: nowrap;
	display:inline;
}

.softphoneArea a {
	margin-left: 2px;
}

.softphoneArea ul {
	float: left;
	height: 29px;
}
.softphoneArea li {
	float: left;
	margin-left: 2px;
	line-height: 29px;
}
.softphoneArea input {
	height: 20px;
	width: 100px;
	margin-top: 2px;
	border: 1px solid #CCCCCC;
	text-align: left;
	line-height: 20px;
	font-size: 14px;

}
.softphoneArea .times {
	font-size: 12px;
	font-weight: bold;
	color: #FFFFFF;
	background-image: url(/images/core/times.gif);
	background-repeat: no-repeat;
	background-position: 2px;
}
.times div {
	text-indent: 35px;
}
#headerT .exit {
	height: 30px;
	float: right;
	width: 110px;
	line-height: 30px;
	color: #FFFFFF;
	font-weight: bold;
	position: absolute;
	right: 10px;
	background-image: url(/images/core/exit.gif);
	background-repeat: no-repeat;
}
.exit div {
	margin-left: 20px;
}
.exit a {
	font-weight: bold;
	font-size:12px;
	color: #003377;!important
}
.runtask{
	background-color: #000000;
	color:#fff;
}
	